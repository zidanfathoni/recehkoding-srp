{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "Portfolio",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "personal",
        "team"
      ],
      "required": true,
      "default": "personal"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "progress",
        "completed"
      ],
      "required": true,
      "default": "pending"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "detail": {
      "type": "component",
      "repeatable": false,
      "component": "content.portfolio-detail",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "required": true
    }
  }
}
