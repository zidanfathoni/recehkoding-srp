{
  "kind": "collectionType",
  "collectionName": "zidanfath_projects",
  "info": {
    "singularName": "zidanfath-project",
    "pluralName": "zidanfath-projects",
    "displayName": "zidanfath-project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "stacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stack.stack"
    },
    "link": {
      "type": "text",
      "default": "#"
    },
    "detail": {
      "type": "component",
      "repeatable": false,
      "component": "content.blog-detail",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
