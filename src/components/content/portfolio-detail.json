{
  "collectionName": "components_content_portfolio_details",
  "info": {
    "displayName": "portfolio-detail"
  },
  "options": {},
  "attributes": {
    "content": {
      "type": "customField",
      "options": {
        "preset": "default"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "stacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::stack.stack"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "start_date": {
      "type": "date",
      "required": true
    },
    "end_date": {
      "type": "date",
      "required": true
    },
    "link": {
      "type": "text",
      "required": true
    }
  }
}
